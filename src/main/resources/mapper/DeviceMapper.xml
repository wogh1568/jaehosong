<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.demo.dao.DeviceDao">	<!-- mapper 인터페이스가 위치하는 경로 -->
    <select id="selectList" parameterType="com.example.demo.domain.params.DeviceParams" resultType="com.example.demo.domain.Device">	<!-- id: mapper 인터페이스에서 선언한 메서드 이름과 같아야 한다. resultType: VO 클래스가 위치하는 경로 -->
        SELECT *
        FROM chartdata	<!-- SQL문 작성 -->
    </select>


</mapper>